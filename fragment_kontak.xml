            android:id="@+id/relativ2"
            >

            <ImageView
                android:id="@+id/gambar1"
                android:layout_width="97dp"
                android:layout_height="128dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="70dp"
                android:src="@drawable/profil" />
            <TextView
